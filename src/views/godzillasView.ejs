<html>

<head>
  <meta charset="utf-8" />
  <title>Godzilla Kaiju Database</title>
  <meta name="generator" content="Bootply" />
  <link rel="stylesheet" href="css/bootstrap.min.css">
  <link rel="stylesheet" href="css/bootstrap-theme.min.css">
  <link rel="stylesheet" href="css/styles.css">
</head>

<body>
  <header>
    <div class="container">
      <div class="navbar-header">
        <h1><a class="navbar-brand"><%=webTitle%></a></h1>
      </div>
      <nav class="collapse navbar-collapse" role="navigation">
        <ul class="nav navbar-nav">
          <%for(let i = 0; i<nav.length; i++) {%>
            <li>
              <a href="<%=nav[i].link%>"><%=nav[i].title%></a>
            </li>
          <%}%>
        </ul>
      </nav>
    </div>
  </header>

  <div class="uppersection">
    <%for(let i = 0; i<godzillaList.length; i++) {%>
      <div class="subsection">
      <h2 class="godzilla-name"><%=godzillaList[i].name%></h2>
      <h3>Appearances</h3>
        <div class="section-contents">
          <ul class="godzilla-list">
            <%for(let j = 0; j<godzillaList[i].appearances.length; j++) {%>
              <li><%=godzillaList[i].appearances[j]%></li>
            <%}%>
          </ul>
        </div>
      </div>
    <%}%>
  </div>
</body>

<script src="js/jquery.min.js"></script>
<script   src="js/bootstrap.min.js"></script>

</html>